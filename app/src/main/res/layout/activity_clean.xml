    <RelativeLayout
        android:id="@+id/rl_more_function"
        android:layout_width="240dp"
        android:layout_height="60dp"
        android:layout_centerInParent="true"
        android:background="@drawable/bg_selector"
        android:visibility="invisible">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            >

            <TextView
                android:id="@+id/btn_more_function"
                android:layout_width="144dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="55dp"
                android:layout_marginRight="10dp"
                android:text="@string/onkeycleandefaultcontent"
                android:textColor="#318cfc"
                android:textSize="14sp"
                />
            <!--android:textColor="#DD2A52D2"-->

            <ImageView
                android:id="@+id/iv_arrow"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/circle_arrow"
                />
            <!--android:src="@drawable/flash_arrow_blue_anim"-->

            <!--<com.ymnet.onekeyclean.cleanmore.customview.MainScrollUpAdvertisementView
                android:id="@+id/suv_more_function"
                android:layout_width="wrap_content"
                android:layout_height="40dp"/>-->
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_anim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        >

        <ImageView
            android:id="@+id/imageView_cylindrical"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_centerInParent="true"
            android:scaleType="centerInside"
            android:src="@mipmap/cylindrical"

            />

        <ImageView
            android:id="@+id/imageView_within"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_centerInParent="true"
            android:scaleType="centerInside"
            android:src="@mipmap/within"/>

        <ImageView
            android:id="@+id/imageView_within1"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_centerInParent="true"
            android:scaleType="centerInside"
            android:src="@mipmap/within_1"/>

        <TextView
            android:id="@+id/memory_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>

        <ImageView
            android:id="@+id/imageView_determine"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_centerInParent="true"
            android:src="@mipmap/determine"
            android:visibility="invisible"/>

        <!--清理软件自定义布局-->
        <com.ymnet.killbackground.view.customwidget.Wheel
            android:id="@+id/wheel_iv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/iv_clean1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean4"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>


            <ImageView
                android:id="@+id/iv_clean5"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean7"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>

            <ImageView
                android:id="@+id/iv_clean10"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@mipmap/robot"
                android:visibility="invisible"/>
        </com.ymnet.killbackground.view.customwidget.Wheel>
    </RelativeLayout>

    <com.ymnet.killbackground.view.customwidget.BoomView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</RelativeLayout>