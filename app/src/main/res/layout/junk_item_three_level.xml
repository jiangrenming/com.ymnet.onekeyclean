<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/ll_three"
              android:layout_width="fill_parent"
              android:layout_height="match_parent"
              android:background="#ffededed"
              android:orientation="vertical">

    <ImageView
        android:id="@+id/junk_item_child_divider"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginLeft="7.0dip"
        android:layout_marginRight="7.0dip"
        android:background="@drawable/junk_item_child_divider"
        />

    <RelativeLayout
        android:id="@+id/front_layout"
        android:layout_width="fill_parent"
        android:layout_height="50.0dip"
        android:layout_gravity="top"
        android:layout_marginLeft="7.0dip"
        android:layout_marginRight="7.0dip"
        android:background="@drawable/junk_list_child_selector">

        <RelativeLayout
            android:id="@+id/cache_type_layout"
            android:layout_width="24.0dip"
            android:layout_height="30.0dip"
            android:layout_marginBottom="10.0dip"
            android:layout_marginLeft="20.0dip"
            android:layout_marginTop="10.0dip">

            <ImageView
                android:id="@+id/cache_type_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:scaleType="fitXY"
                android:src="@drawable/junk_app_logo_cache"/>
        </RelativeLayout>

        <TextView
            android:id="@+id/cache_type_name"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="10.0dip"
            android:layout_toLeftOf="@+id/cache_type_size"
            android:layout_toRightOf="@id/cache_type_layout"
            android:textColor="#ff666666"
            android:textSize="14.0sp"/>

        <TextView
            android:id="@+id/cache_type_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/cache_type_check"
            android:textColor="#ff666666"
            android:textSize="13.0sp"/>

        <ImageView
            android:id="@+id/cache_type_check"
            android:layout_width="40.0dip"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="#00ffffff"
            android:scaleType="center"
            android:src="@drawable/junk_check_box_bg"/>


    </RelativeLayout>

</LinearLayout>